{
  "root": true,
  "extends": [
    "eslint:recommended",
    "plugin:vue/base",
    "plugin:vue/recommended",
    "plugin:vue/strongly-recommended"
  ],
  "globals": {
    "Drupal": true,
    "drupalSettings": true,
    "drupalTranslations": true,
    "domready": true,
    "jQuery": true,
    "_": true
  },
  "rules": {
    "semi": [2, "always", { "omitLastInOneLineBlock": true}]
  }
}
