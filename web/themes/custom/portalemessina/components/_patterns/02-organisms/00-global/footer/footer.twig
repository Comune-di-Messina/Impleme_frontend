{% set dist_url = dist_url|default('/themes/custom/portalemessina/dist/') %}

<footer class="it-footer" role="contentinfo">
  <div class="it-footer-main">
    <div class="container">
      <section class="it-footer-logos py-4">
        <div class="row">
          <div class="col">
            {% block nav_logos %}
              {{ footer_logos }}
            {% endblock %}
          </div>
        </div>
      </section>
      <section class="it-footer-primary py-4">
        <div class="row">
          <div class="col">
            {% block nav_main %}
              {{ footer_main }}
            {% endblock %}
          </div>
        </div>
      </section>
      <section class="it-footer-secondary py-4">
        <div class="row">
          <div class="col-lg-4 col-md-6">
            {% block nav_contacts %}
              {{ footer_main }}
            {% endblock %}
          </div>
          <div class="col-lg-4 col-md-6">
            {% block nav_socials %}
              {{ footer_main }}
            {% endblock %}
          </div>
        </div>
      </section>
    </div>
    <div class="it-footer-small-prints clearfix">
      <div class="container">
        {% block nav_secondary %}
          {{ footer_secondary }}
        {% endblock %}
      </div>
    </div>
  </div>
</footer>