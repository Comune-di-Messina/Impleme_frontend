msgstr "Stabile"

msgid "The language code."
msgstr "Il codice della lingua."

msgid "Delete %label"
msgstr "Elimina %label"

msgid "Editing languages"
msgstr "Modificare le lingue"

msgid "Max 1300x1300"
msgstr "Massimo 1300x1300"

msgid "Max 2600x2600"
msgstr "Massimo 2600x2600"

msgid "Max 325x325"
msgstr "Massimo 325x325"

